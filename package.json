{
  "name": "smart-investing-ideas-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": " ng  jest",
    "test:watch": " ng jest --watch",
    "test:coverage": " ngjest --coverage",
    "generate-api": "openapi-generator-cli generate -i open-api-yaml/stock.yaml -g typescript-angular -o src/generated/ --global-property models"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.0",
    "@angular/cdk": "^15.2.0",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/forms": "^15.2.10",
    "@angular/material": "^15.2.9",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/router": "^15.2.0",
    "@ngrx/component-store": "^15.2.0",
    "@types/jest": "^29.5.14",
    "ag-grid-angular": "^31.3.4",
    "ag-grid-community": "^31.3.4",
    "ag-grid-enterprise": "^31.3.4",
    "angular-plotly.js": "5.0.0",
    "angular-svg-icon": "^15.0.0",
    "install": "^0.13.0",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.5.2",
    "lodash": "^4.17.21",
    "npm": "^10.9.2",
    "openapi-typescript": "^6.7.5",
    "openapi-typescript-codegen": "^0.28.0",
    "plotly.js-dist-min": "^2.26.0",
    "rxjs": "^7.4",
    "tslib": "^1.14.1",
    "zone.js": "~0.11.4"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setup.jest.ts"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/dist/"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>/tsconfig.spec.json",
        "stringifyContentPathRegex": "\\.html$"
      }
    }
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.11",
    "@angular/cli": "^15.2.0",
    "@angular/compiler-cli": "^15.2.0",
    "@ngx-formly/schematics": "^6.3.12",
    "@openapitools/openapi-generator-cli": "^2.13.2",
    "@types/jasmine": "^3.10.18",
    "@types/lodash": "^4.17.15",
    "@types/node": "^12.11.1",
    "@types/plotly.js-dist-min": "^2.3.1",
    "autoprefixer": "^10.4.20",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15",
    "typescript": "~4.9.5"
  }
}
